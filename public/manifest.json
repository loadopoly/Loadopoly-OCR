{
  "id": "/",
  "name": "GeoGraph Node",
  "short_name": "GeoGraph",
  "description": "Advanced OCR-to-Graph platform for historical data extraction and GIS metadata generation.",
  "lang": "en",
  "dir": "ltr",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "background_color": "#020617",
  "theme_color": "#020617",
  "orientation": "any",
  "categories": [
    "education",
    "productivity",
    "utilities"
  ],
  "prefer_related_applications": false,
  "icons": [
    {
      "src": "/icon.svg",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "/icon.svg",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "/icon.svg",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "maskable"
    }
  ],
  "shortcuts": [
    {
      "name": "Quick Process",
      "short_name": "Process",
      "description": "Start a new high-throughput batch scan",
      "url": "/?tab=batch",
      "icons": [
        {
          "src": "/icon.svg",
          "sizes": "192x192",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "AR Scanner",
      "short_name": "AR",
      "description": "Open the immersive AR field scanner",
      "url": "/?tab=ar",
      "icons": [
        {
          "src": "/icon.svg",
          "sizes": "192x192",
          "type": "image/svg+xml"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "name",
      "text": "description",
      "files": [
        {
          "name": "media",
          "accept": [
            "image/*",
            "application/pdf"
          ]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "image/png": [".png"],
        "image/jpeg": [".jpg", ".jpeg"],
        "application/pdf": [".pdf"]
      }
    }
  ],
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "display_override": [
    "window-controls-overlay",
    "standalone",
    "browser"
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "protocol_handlers": [
    {
      "protocol": "web+geograph",
      "url": "/?graph=%s"
    }
  ],
  "note_taking": {
    "new_note_url": "/?note=new"
  },
  "scope_extensions": [
    {
      "origin": "https://geographocrnode.vercel.app"
    }
  ],
  "related_applications": [],
  "widgets": [
    {
      "name": "GeoGraph Status",
      "description": "Check your current OCR processing status",
      "tag": "status",
      "template": "status-widget",
      "ms_ac_template": "widgets/status.json",
      "data": "widgets/status-data.json",
      "type": "application/json",
      "screenshots": [
        {
          "src": "/icon.svg",
          "sizes": "192x192",
          "label": "Widget Preview"
        }
      ],
      "icons": [
        {
          "src": "/icon.svg",
          "sizes": "192x192"
        }
      ]
    }
  ]
}
