{
  "name": "geograph-ocr-node",
  "version": "2.4.0",
  "description": "Advanced OCR-to-Graph platform with GIS metadata, semantic NLP, deduplication, and metaverse navigation",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:extension": "npm run build && npm run copy:extension",
    "copy:extension": "cp public/manifest.json dist/ && cp -r public/icons dist/ && cp public/privacy-policy.html dist/",
    "zip:extension": "cd dist && zip -r ../geograph-ocr-extension.zip . -x '*.map'",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "@google/genai": "^1.0.0",
    "@supabase/supabase-js": "^2.45.4",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "d3": "^7.9.0",
    "dexie": "^4.0.1",
    "ethers": "^6.13.4",
    "https-browserify": "^1.0.0",
    "lucide-react": "^0.559.0",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-force-graph-2d": "^1.25.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.3",
    "util": "^0.12.5",
    "uuid": "^10.0.0"
  },
  "optionalDependencies": {
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^9.0.0",
    "three": "^0.172.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.172.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  }
}
